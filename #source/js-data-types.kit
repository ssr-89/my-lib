						<button class="accordion__title btn" data-spoller>Boolean</button>
					<div class="accordion__content">
						<div class="accordion__wrapper">
							<pre class="accordion__pre block">
								<code class="accordion__code block">
var primer = 100 > 50; <i class="accordion__i">// true или false</i>
console.log(primer);
console.log(typeof primer);

let x = 10;
let y = 20;

if (x > y) {
  console.log("x больше y");
}

if (x < y) {
  console.log("x меньше y");
}
								</code>
							</pre>
						</div>
					</div>
					</div>
				</div>
			</div>
